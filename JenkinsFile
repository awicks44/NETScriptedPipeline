pipeline{
    agent any   
   
    stages {
    	stage('Checkout') {
    		steps {
    			git credentialsId: '30ea8398-b185-414a-9429-6075ce56bab7', url: 'git@github.com:awicks44/NETScriptedPipeline.git'
    		}    		
    	}

    	stage('Clean') {
    		steps {
    			echo 'CLEANING'
    			bat "dotnet clean JenkinsConsoleApp.sln"
			}
    	}

    	stage('Build') {
    		steps {
    			echo 'BUILDING'
    			bat "dotnet build JenkinsConsoleApp.sln --configuration Release"
    		}
    	}

    	stage('Publish') {
    		steps {
    			echo 'PUBLISHING'
    			bat "dotnet publish JenkinsConsoleApp.sln"
    		}
    	}
    }

    post {
		success {
			echo 'Successful Build'
		}
	}
    
 }